{
  "name": "avocado-service",
  "version": "1.0.0",
  "description": "avocado-service",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node ./src\"",
    "build": "babel src -s -D -d dist",
    "start": "NODE_ENV=production node dist",
    "prestart": "npm run -s build",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./test/coverage/lcov.info | coveralls",
    "lint": "eslint src test --ignore-pattern test/coverage*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ollelauribostrom/avocado-service.git"
  },
  "keywords": [],
  "author": "Olle Lauri Bostr√∂m (https://github.com/ollelauribostrom)",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-rate-limit": "^2.11.0",
    "helmet": "^3.9.0",
    "mongoose": "^5.0.9",
    "morgan": "^1.9.0",
    "rotating-file-stream": "^1.3.4",
    "shortid": "^2.2.12"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^23.4.2",
    "nodemon": "^1.12.5",
    "supertest": "^3.1.0"
  },
  "bugs": {
    "url": "https://github.com/ollelauribostrom/avocado-service/issues"
  },
  "homepage": "https://github.com/ollelauribostrom/avocado-service#readme",
  "now": {
    "name": "avocado-service",
    "alias": "avocado-service.now.sh",
    "dotenv": true,
    "public": true,
    "type": "docker"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/test/coverage",
    "testURL": "http://localhost"
  }
}
